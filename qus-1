#include <stdio.h>
#include <stdlib.h>

int main() {
    int n,k;
    scanf("%d", &n);
    
    int *arr = malloc(n * sizeof(int));
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    int pos, x;
    scanf("%d", &pos);
    scanf("%d", &x);
    
    pos--;
    
    arr = realloc(arr, (n + 1) * sizeof(int));
    
    for(int i = n; i > pos; i--) {
        arr[i] = arr[i - 1];
    }
    
    arr[pos] = x;
    
    for(int i = 0; i <= n; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");
    
    free(arr);
    return 0;
}
